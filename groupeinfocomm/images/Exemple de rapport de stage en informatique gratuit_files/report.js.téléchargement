(function(b){b(document).ready(function(){var c;c=1<reportUrl.split("http").length?'<iframe style="border: 0;width: 100%;height: 100%" src='+reportUrl+"></iframe>":reportUrl;modalService.createModalPlease(Translator.trans("script.report.modal.title"),c,Translator.trans("script.report.modal.action.back"),"","reportPop","reportPop");b("#reportClick").on("click",function(a){a.preventDefault();a.stopPropagation();modalService.toggleModalPlease("reportPop","show")});b("#reportPop #valid").on("click",function(){modalService.toggleModalPlease("reportPop",
"hide")});b("body").on("click",".report-comment",function(a){a.preventDefault();a.stopPropagation();a=b(this).attr("href");c=1<a.split("http").length?'<iframe style="border: 0;width: 100%;height: 100%" src='+a+"></iframe>":a;modalService.createModalPlease(Translator.trans("script.report.modal.title"),c,Translator.trans("script.report.modal.action.back"),"","reportComment","reportComment");modalService.toggleModalPlease("reportComment","show")});b("body").on("click","#reportComment #valid",function(){modalService.removeModalPlease("reportComment")})})})(window.jQuery);
